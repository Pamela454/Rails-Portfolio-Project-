<%= form_for(@response) do |f| %>
<h1>New Response</h1>

<div class="form-group">
    <%= f.label :response %><br>
    <%= f.text_field :response, class: "form-control" %></div><br><br>

<div class="form-group">
    <%= f.label :message_id %><br>
    <%= f.hidden_field :message_id, class: "form-control" %></div><br><br>

<div class="form-group">
    <%= f.label :physician_id %><br>
    <%= f.hidden_field :physician_id, class: "form-control" %></div><br><br>

<div class="actions">
    <%= f.submit class: "btn btn-primary" %>
</div><br>

    <%= link_to 'Log Out', '/logout', :method => :delete  %>


<% end %>
